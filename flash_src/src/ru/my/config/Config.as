package ru.my.config {	import flash.events.EventDispatcher;	public final class Config extends EventDispatcher {		// private		private var _data : Object = null;		// singleton warner		public function Config() {			if ( instance ) throw new Error("Settings is singleton and it can only be accessed through Settings.getInstance()");		}		// signleton instance 		private static var instance : Config = new Config();		// signleton instance getter		public static function getInstance() : Config {			return instance;		}		public function set data(data : Object) : void {			_data = data;		}		public function get data() : Object {			return _data;		}		public function reset() : void {			_data = null;		}	}}